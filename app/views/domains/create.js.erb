<% if @domains.blank? %>
	$('#ui-tabs-1 .dashboard_content').html("<%= escape_javascript(render(:partial => 'new')) %>");
<% else %>
	$('#ui-tabs-1 .dashboard_sidebar').replaceWith("<%= escape_javascript(render(:partial => 'dashboard_sidebar')) %>");
	$('#ui-tabs-1 .dashboard_content').html("<%= escape_javascript(render(:partial => 'show')) %>");
<% end %>